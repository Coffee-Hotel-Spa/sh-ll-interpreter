fn DerefTest()
    let program = "../../../file.Ã¦"
    let file1 = @program
    A:assertType(file, file1, "Dereferencing a user function should return a user function")
    let result = file1()
    A:assertType(result, string, "Dereferencing a user function should return a string")
end

A:describe("Test dereference operator", DerefTest)

let testFile1Path = "../../../TestFiles/testfile1.txt"

A:describe("Read a file", fn()
    let file = @testFile1Path;
    
    A:assertType("file", file, "Dereferencing a string should return a file")

    let content = file.readToEnd()
    
    
end)